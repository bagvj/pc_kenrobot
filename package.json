{
  "name": "kenrobot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --win --ia32 --dir",
    "dist": "build --win --ia32",
    "release": "build --win --ia32"
  },
  "build": {
    "appId": "com.kenrobot.KenRobot",
    "files": [
      "assets",
      "index.html",
      "main.js",
      "renderer.js",
      "node_modules"
    ],
    "extraFiles": [
      "arduino-${os}",
      "scripts"
    ]
  },
  "devDependencies": {
    "7zip-bin": "^2.0.4",
    "electron": "^1.4.12",
    "electron-builder": "^10.8.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "minimist": "^1.2.0",
    "nan": "^2.4.0",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
